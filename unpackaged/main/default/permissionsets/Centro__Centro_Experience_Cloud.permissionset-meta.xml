<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Grants Experience Cloud users to use Centro LWC.</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Centro Experience Cloud Users</label>
    <classAccesses>
        <apexClass>Centro__CentroAPI</apexClass>
        <enabled>true</enabled>
    </classAccesses>
    <classAccesses>
        <apexClass>Centro__LinkedChannelsController</apexClass>
        <enabled>true</enabled>
    </classAccesses>
    <classAccesses>
        <apexClass>Centro__SetupController</apexClass>
        <enabled>true</enabled>
    </classAccesses>
</PermissionSet>
