            <apex:variable value="{!OR($User.UIThemeDisplayed == 'Theme4d', $User.UIThemeDisplayed == 'Theme4t')}" var="isLightning" />
            <apex:includeScript value="{!URLFOR($Resource.breadwinner_ns__Breadwinner_for_NetSuite, 'Breadwinner/Scripts/jquery-3.5.1.min.js')}"/>
            <apex:includeScript value="{!URLFOR($Resource.breadwinner_ns__Breadwinner_for_NetSuite, 'Breadwinner/Scripts/gstatic.min.js')}" />
            <script type="text/javascript">
            var j$ = jQuery.noConflict();
            j$(function(){
                message();
                imporoveSFLookupStyles();
                if(document.getElementsByTagName("title") && document.getElementsByTagName("title")[0]){ 
                    document.getElementsByTagName("title")[0].innerHTML = "Employee: {!JSENCODE(emp.Name)}";
                }
            });
            function imporoveSFLookupStyles(){
                
                j$("a[id*='chosenSFUser_lkwgt']").removeAttr("class");
                
                // User specific lookup style fixes
                j$("div[id*='chosenSFUser_top']").removeAttr("class").addClass("slds-grid");
                j$("select[id*='chosenSFUser_mlktp']").addClass("slds-select inheritWidth");
                j$("input[id*='chosenSFUser']").addClass("slds-input inheritWidth");
            }
            function message(){
                j$(".infoM6, .infoM4, .infoM3, .infoM2, .infoS1").addClass("slds-notify slds-notify_alert slds-theme_inverse-text  slds-text-align_left slds-text-heading_small slds-m-bottom_small").css( {"font-weight":"500","line-height":"1.4","word-spacing":"1px","background-color":"rgba(84,105,141,.95)","border-color":"rgba(84,105,141,.95)"});
                j$(".errorM6, .errorM4, .errorM3, .errorM2, .errorS1").addClass("slds-notify slds-notify_alert slds-theme_error  slds-text-align_left slds-text-heading_small slds-m-bottom_small").css({"font-weight":"500","line-height":"1.4","word-spacing":"1px","display":"block","border-radius":"5px"});
                j$(".warningM6, .warningM4, .warningM3, .warningM2, .warningS1").addClass("slds-notify slds-notify_alert slds-theme_warning  slds-text-align_left slds-text-heading_small slds-m-bottom_small").css( {"font-weight":"500","line-height":"1.4","word-spacing":"1px"} ) ;
                j$(".confirmM6, .confirmM4, .confirmM3, .confirmM2, .confirmS1").addClass("slds-notify slds-notify_alert slds-theme_success slds-text-align_left slds-text-heading_small slds-m-bottom_small").css( {"font-weight":"500","line-height":"1.4","word-spacing":"1px"} ) ;
                
                j$("div .messageText h4").css( "color", "white" );
                j$("div[class*='warning'] div.messageText h4").css( "color", "black" );
                j$("table.messageTable td").css({"color" : "white"});
                j$("div[class*='warning'] table.messageTable td").css( "color", "black" );
            }
            function showHideSectionConent(sectionId,sectionContentId){
                var sectiondiv=document.getElementById(sectionId);
                j$("#"+sectionContentId).show();
                if(j$(sectiondiv).hasClass("slds-is-open")){
                    j$(sectiondiv).removeClass("slds-is-open");
                }
                else{
                    j$(sectiondiv).addClass("slds-is-open");
                }
            }
            function reRenderImage(){
                var imageURL = '{!URLFOR($Asset.SLDS, 'assets/icons/custom-sprite/svg/symbols.svg#custom3')}';
                var SVG = j$('<svg/>', {
                    class: 'slds-icon slds-icon slds-page-header__icon',
                });
                var SVGUse = j$('<use/>');
                SVGUse.attr('xlink:href',imageURL);
                j$('.Icon').prepend(SVG.append(SVGUse));
                j$('.Icon' ).html(j$('.Icon').html());
            }

            google.load('visualization', '1', {packages:['orgchart']});
            google.charts.setOnLoadCallback(drawChart);
            function drawChart(){
                var data = new google.visualization.DataTable();
                data.addColumn('string', 'Name');
                data.addColumn('string', 'Name');
                data.addColumn('string', 'ToolTip');
                /*Getting essentials from Apex class*/
                var orgchart=JSON.parse('{!JSENCODE(orgChart)}');
                var empcount='{!employeeCount}';
                var sub='{!subcount}';
                var name='{!JSENCODE(emp.Name)}';
                var jobInforamtion=JSON.parse('{!JSENCODE(jobInformation)}');
                var previous='{!JSENCODE(topEmployeeName)}';
                
                if(empcount!=0){
                    console.log(orgchart);
                    console.log(jobInforamtion);
                    var c=0;
                    /*Arranging values into charts*/
                    for(var single in orgchart){
                        if(previous==single){ 
                            for(i=0;i<orgchart[single].length;i++){
                                if(single==name){
                                    
                                    data.addRows([
                                        [{'v':String(single), 'f':'<div style="text-align: center;background:lightgreen;"><b>'+String(single)+'</b>'+''+'<div style="text-align: center;color:#838383;font-style:italic;">'+jobInforamtion[String(single)]+'</div></div>'},'',''],
                                        [{'v':String(orgchart[single][i]), 'f':'<div style="text-align: center;"><b>'+String(orgchart[single][i])+'</b></div>'+''+'<div style="text-align: center;color:#838383;font-style:italic;">'+jobInforamtion[String(orgchart[single][i])]+'</div>'}
                                         ,String(single),'']
                                    ]); c=c+1; 
                                }  
                                else if(orgchart[single][i]==name){
                                    
                                    data.addRows([
                                        [{'v':String(single), 'f':'<div style="text-align: center;"><b>'+String(single)+'</b>'+''+'<div style="text-align: center;color:#838383;font-style:italic;">'+jobInforamtion[String(single)]+'</div></div>'},'',''],
                                        [{'v':String(orgchart[single][i]), 'f':'<div style="text-align: center;background:lightgreen;"><b>'+String(orgchart[single][i])+'</b>'+''+'<div style="text-align: center;color:#838383;font-style:italic;">'+jobInforamtion[String(orgchart[single][i])]+'</div></div>'}
                                         ,String(single),'']
                                    ]); c=c+1;
                                    
                                }
                                    else{
                                        
                                        data.addRows([
                                            [{'v':String(single), 'f':'<div style="text-align: center;"><b>'+String(single)+'</b>'+''+'<div style="text-align: center;color:#838383;font-style:italic;">'+jobInforamtion[String(single)]+'</div></div>'},'',''],
                                            [{'v':String(orgchart[single][i]), 'f':'<div style="text-align: center;"><b>'+String(orgchart[single][i])+'</b></div>'+''+'<div style="text-align: center;color:#838383;font-style:italic;">'+jobInforamtion[String(orgchart[single][i])]+'</div>'}
                                             ,String(single),'']
                                        ]); c=c+1;
                                    }
                            }
                        }
                        else{
                            for(i=0;i<orgchart[single].length;i++){
                                if(orgchart[single][i]==name){
                                    data.addRows([
                                        
                                        [{'v':String(orgchart[single][i]), 'f':'<div style="text-align: center;background:lightgreen;"><b>'+String(orgchart[single][i])+'</b>'+''+'<div style="text-align: center;color:#838383;font-style:italic;">'+jobInforamtion[String(orgchart[single][i])]+'</div></div>'}
                                         ,String(single),'']
                                    ]);c=c+1; 
                                }
                                else{
                                    data.addRows([
                                        
                                        [{'v':String(orgchart[single][i]), 'f':'<div style="text-align: center;"><b>'+String(orgchart[single][i])+'</b>'+''+'<div style="text-align: center;color:#838383;font-style:italic;">'+jobInforamtion[String(orgchart[single][i])]+'</div></div>'}
                                         ,String(single),'']
                                    ]);c=c+1; 
                                }
                            }
                        }
                        console.log('executed');
                    } 
                    console.log('missed');
                    for(var i=0;i<=c;i++){
                        
                        data.setRowProperty(i,'style','border:none');
                    }
                    var chart = new google.visualization.OrgChart(document.getElementById('chart_div'));
                    chart.draw(data, {'allowHtml':true});
                }
            }
            </script>
