                                                            <apex:outputPanel rendered="{!nsEmployee.breadwinner_ns__Salesforce_User__c!=NULL}">
                                                                associated with Salesforce User <b>{!nsEmployee.Salesforce_User__r.Name}</b>.
                                                            </apex:outputPanel>
                                                            <apex:outputPanel rendered="{!nsEmployee.breadwinner_ns__Salesforce_User__c==NULL}">
                                                                not associated with any Salesforce User.
                                                            </apex:outputPanel>                                                        
                                                        </apex:outputPanel>
                                                        <div class="slds-text-heading_small slds-m-bottom_none slds-p-right_medium slds-p-top_xxx-small slds-large-size_1-of-1 slds-medium-size_1-of-1 slds-grid">
                                                            <div class="slds-text-heading_small slds-m-bottom_none slds-p-right_medium slds-p-top_xxx-small slds-large-size_2-of-4 slds-medium-size_2-of-4">
                                                                Do you want this NetSuite Employee be associated with a Salesforce User?
                                                            </div>
                                                            <div class="slds-form-element">   
                                                                <label class="slds-checkbox_toggle slds-grid"> 
                                                                    <apex:inputCheckbox id="DoNotAssociateSFUser" value="{!hasAssociatedSFUser}">
                                                                        <apex:actionSupport event="onclick" status="action-status" reRender="sfUserLookupPanel" oncomplete="imporoveSFLookupStyles();" />
                                                                    </apex:inputCheckbox>
                                                                    <span  class="slds-checkbox_faux_container" aria-live="assertive">
                                                                        <span class="slds-checkbox_faux"></span>
                                                                        <span class="slds-checkbox_on">Yes</span>
                                                                        <span class="slds-checkbox_off">No</span>
                                                                    </span>
                                                                </label>
                                                            </div>
                                                        </div>
                                                        <apex:outputPanel layout="block" id="sfUserLookupPanel">
                                                            <apex:outputPanel styleClass="slds-grid" layout="block" rendered="{!hasAssociatedSFUser}">
                                                                <label class="slds-text-heading_small slds-p-top_xx-small" for="chosenSFUser">Choose Salesforce User</label>
                                                                <div class="slds-form-element__control slds-p-left_small">
                                                                    <apex:inputField id="chosenSFUser" styleClass="slds-input accountInput slds-large-size_1-of-1" value="{!nsEmployee.breadwinner_ns__Salesforce_User__c}" style="width:250px;"/>
                                                                </div>
                                                            </apex:outputPanel>
                                                        </apex:outputPanel>
                                                    </apex:outputPanel>
                                                </apex:outputPanel>
                                            </div>
                                        </div>
                                    </apex:outputPanel>
                                    <apex:outputPanel id="panelbuttons" layout="block" styleClass="slds-m-top_small slds-m-bottom_medium slds-text-align_center">
										<apex:commandButton value="Cancel" styleClass="slds-button slds-button_neutral slds-m-right_x-small" action="{!Cancel}" status="action-status" id="cancelbutton"/>
                                        <apex:commandButton value="Save" styleClass="slds-button slds-button_brand" id="savebutton" status="action-status" action="{!changeEmployeeSyncStatus}" oncomplete="message();imporoveSFLookupStyles();" disabled="{!OR(NOT($ObjectType.breadwinner_ns__BW_Employee__c.Updateable),ISNULL(appConfig))}" reRender="pm,controlstatus"/>
                                    </apex:outputPanel>
                                </apex:outputPanel>
                            </apex:outputPanel>
                        </apex:outputPanel>
                    </apex:outputPanel>
                </apex:form>
            </div>
        </body>
    </html>
</apex:page>