                        <apex:actionFunction name="sorting" action="{!queryNSContacts}" reRender="contactTablePanel,buttonsPanel">
                            <apex:param name="sortExp" value="" assignto="{!sortExpression}" />
                        </apex:actionFunction>
                        <apex:inputHidden id="primaryContactId" value="{!primaryContactInternalId}"></apex:inputHidden>
                        <apex:actionFunction status="action-status" name="showPrimaryContactError" oncomplete="message();showHideModal('pcError_popup','pcError_backdrop');" reRender="pcErrorSection"/>
                        <apex:actionFunction action="{!redirectAfterPrimaryContactError}" status="action-status" name="redirectAfterPrimaryContactError" reRender="OverallPage,contactModalContent,pcErrorSection,estimateModalContent" oncomplete="showHideModal('pcError_popup','pcError_backdrop');setPageTitle();message();window.scrollTo(0,0);reRenderCheckIcons(['#stage1','#stage2','#stage3']);reRenderModalCancelSVGIcons('billingAddressPopUpCloseIcon');reRenderModalCancelSVGIcons('shippingAddressPopUpCloseIcon');reRenderSalesOrderSVG();DatepickerOnfocusToOnclick();initSelect2();setClassicItemTableLayout();"/>

						<apex:actionFunction name="selectEstimate" status="action-status" action="{!queryNSEstimates}" reRender="estimateModalContent" oncomplete="showHideModal('estimateModal','estimateModalBackdrop');message();"/>
						<apex:inputHidden id="estimateId" value="{!sfSalesOrder.breadwinner_ns__CreatedFromInternalId__c}"></apex:inputHidden>

                    </apex:outputPanel>
                    <!-- Contact Modal Start -->
                    <apex:outputPanel layout="block" id="nsContactSection">
                        <div class="slds-modal slds-modal_large" aria-hidden="false" role="dialog" id="contactModal">
                            <div class="slds-modal__container" style="width:100%;padding:2rem;">
                                <div class="slds-modal__header">
                                    <button id="contactModalCanBtn" class="slds-button slds-button_icon-inverse slds-modal__close" type="button" onclick="showHideModal('contactModal','contactModalBackdrop');">
                                        <svg aria-hidden="true" class="slds-button__icon slds-button__icon_large" >
                                            <use rel="noopener noreferrer" xlink:href="{!URLFOR($Asset.SLDS, 'assets/icons/action-sprite/svg/symbols.svg#close')}" ></use>
                                        </svg>
                                        <span class="slds-assistive-text">Close</span>
                                    </button>
                                    <h2 class="slds-text-heading_medium">Choose a Primary Contact</h2>
                                </div>
                                <apex:outputPanel layout="block" styleClass="slds-modal__content slds-p-around_medium" id="contactModalContent">
                                    <apex:outputPanel layout="block" rendered="{!AND(transactionPCFeature == '',showCreateTransactionPanel)}" styleClass="slds-p-around_small">

                                    Breadwinner will make it the Primary Contact in NetSuite. However, this information will not be displayed in Salesforce. To reflect this back to Salesforce, you must make a small customization to your NetSuite Org. Please provide&nbsp;<apex:outputLink rel="noopener noreferrer" styleClass="grayLinks" id="LinkToPCKBPage1" value="https://help-netsuite.breadwinner.com/support/solutions/articles/44002187129-how-to-specify-your-netsuite-primary-contact-in-salesforce-using-breadwinner" target="_blank">this</apex:outputLink> help article to your NetSuite and Salesforce Admins.

                                    </apex:outputPanel>
                                    <apex:pageMessage severity="Warning" rendered="{!AND(isEditCompany,showCreateOrEditCompanyPanel)}" summary="Due to limitations in NetSuite’s API, it is not possible for Breadwinner to see if a NetSuite Company already has a Primary Contact. By selecting a Primary Contact below, you might be replacing an existing Primary Contact." id="editcompany-pm"></apex:pageMessage>
                                    <apex:outputPanel layout="block" styleClass="slds-p-bottom_small" rendered="{!queryCustomerContacts.size>0}">
                                        <div class="modalSectionTitle">NetSuite Contacts already associated with this NetSuite Customer</div>
                                        <apex:outputPanel layout="block" styleClass="slds-p-around_small">
                                            <apex:dataTable value="{!queryCustomerContacts}" var="custcon" id="customerContactsTable" styleClass="slds-table slds-table_bordered slds-max-medium-table_stacked-horizontal">
                                                <apex:column headerClass="slds-text-heading_label" headervalue="Name" html-data-label="Name">
                                                    <apex:commandlink rel="noopener noreferrer" value="{!custcon.Name}" onclick="setPrimaryContact('{!JSENCODE(custcon.Name)}','{!JSENCODE(custcon.breadwinner_ns__InternalId__c)}',false)" reRender="nothing"/>
                                                </apex:column>
                                                <apex:column headerClass="slds-text-heading_label" headervalue="Title" html-data-label="Title" value="{!custcon.breadwinner_ns__Title__c}"/>
                                                <apex:column headerClass="slds-text-heading_label" headervalue="Phone" html-data-label="Phone" value="{!custcon.breadwinner_ns__Phone__c}"/>
                                                <apex:column headerClass="slds-text-heading_label" headervalue="Email" html-data-label="Email" value="{!custcon.breadwinner_ns__Email__c}"/>
                                            </apex:dataTable>
                                        </apex:outputPanel>
                                    </apex:outputPanel>
                                    <apex:outputPanel layout="block" styleClass="slds-p-bottom_small" rendered="{!queryNSContactsForSelectedAccount.size>0}">
                                        <div class="modalSectionTitle"><apex:outputText value="{!IF(queryCustomerContacts.size>0,' Other','')} NetSuite Contacts matched with SF Contacts on the Account"/></div>
                                        <apex:outputPanel layout="block" styleClass="slds-p-around_small">
                                            <apex:dataTable value="{!queryNSContactsForSelectedAccount}" var="nsCon" id="matchedContactsTable" styleClass="slds-table slds-table_bordered slds-max-medium-table_stacked-horizontal">
                                                <apex:column headerClass="slds-text-heading_label" headervalue="Name" html-data-label="Name">
                                                    <apex:commandlink rel="noopener noreferrer" value="{!nsCon.nsContact.Name}" onclick="setPrimaryContact('{!JSENCODE(nsCon.nsContact.Name)}','{!JSENCODE(nsCon.nsContact.breadwinner_ns__InternalId__c)}',false)" reRender="nothing"/>
                                                </apex:column>
                                                <apex:column headerClass="slds-text-heading_label" headervalue="Company Name" html-data-label="Company Name" value="{!nsCon.nsContact.Company__r.Name}"/>
                                                <apex:column headerClass="slds-text-heading_label" headervalue="Salesforce Contact" html-data-label="Salesforce Contact">
                                                    <apex:outputText value="{!nsCon.nsContact.Salesforce_Contact__r.Salutation} {!nsCon.nsContact.Salesforce_Contact__r.FirstName} {!nsCon.nsContact.Salesforce_Contact__r.LastName} {!IF(nsCon.IsPrimary,'(Primary)','')}" rendered="{!nsCon.nsContact.breadwinner_ns__Salesforce_Contact__c!=NULL}"/>
                                                    <apex:outputText value="{!nsCon.sfContact.Salutation} {!nsCon.sfContact.FirstName} {!nsCon.sfContact.LastName} {!IF(nsCon.IsPrimary,'(Primary)','')}"  rendered="{!nsCon.nsContact.breadwinner_ns__Salesforce_Contact__c==NULL}"/>
                                                </apex:column>
                                                <apex:column headerClass="slds-text-heading_label" headervalue="Title" html-data-label="Title" value="{!nsCon.nsContact.breadwinner_ns__Title__c}"/>
                                                <apex:column headerClass="slds-text-heading_label" headervalue="Phone" html-data-label="Phone" value="{!nsCon.nsContact.breadwinner_ns__Phone__c}"/>
                                                <apex:column headerClass="slds-text-heading_label" headervalue="Email" html-data-label="Email" value="{!nsCon.nsContact.breadwinner_ns__Email__c}"/>
                                            </apex:dataTable>
                                        </apex:outputPanel>
                                    </apex:outputPanel>
                                    <apex:outputPanel layout="block">
                                        <div class="modalSectionTitle">All NetSuite Contacts</div>
                                        <apex:outputPanel layout="block" styleClass="slds-p-around_small">
                                            <div class="slds-form-element slds-size_1-of-1 slds-medium-size_2-of-3 slds-large-size_1-of-3 slds-m-bottom_medium" style="text-align:left;">
                                                <label class="slds-form-element__label">Contact Name:</label>
                                                <div class="slds-form-element__control">
                                                    <apex:inputText label="" value="{!searchContact}" html-placeholder="Search by Name" id="searchContact" styleClass="slds-input">
                                                        <apex:actionSupport event="onkeyup" action="{!queryNSContacts}" reRender="contactTablePanel,buttonsPanel">
                                                            <apex:param name="contactsearch" value="{!searchContact}" />
                                                        </apex:actionSupport>
                                                    </apex:inputText>
                                                </div>
                                            </div>
                                            <apex:actionFunction name="refreshPageSize" action="{!refreshPageSize}" status="assign-action-status" reRender="contactTablePanel,buttonsPanel"/>
                                            <apex:outputPanel id="contactTablePanel">
                                                <apex:dataTable value="{!ContactsList}" var="con" id="contactTable" styleClass="slds-table slds-table_bordered slds-max-medium-table_stacked-horizontal">
                                                    <apex:column headerClass="slds-text-heading_label" headervalue="Name" html-data-label="Name">
                                                        <apex:facet name="header"><apex:commandLink rel="noopener noreferrer" onclick="sorting('Name');" reRender="nothing" style="color:rgb(81, 79, 77);">Name {!IF(sortExpression=='Name',IF(sortDirection='ASC','▼','▲'),'')}</apex:commandLink></apex:facet>
                                                        <apex:commandlink rel="noopener noreferrer" value="{!con.Name}" onclick="setPrimaryContact('{!JSENCODE(con.Name)}','{!JSENCODE(con.breadwinner_ns__InternalId__c)}',true)" reRender="nothing"/>
                                                    </apex:column>
                                                    <apex:column headerClass="slds-text-heading_label" headervalue="Company Name" html-data-label="Company Name" value="{!con.Company__r.Name}"/>
                                                    <apex:column headerClass="slds-text-heading_label" headervalue="Title" html-data-label="Title" value="{!con.breadwinner_ns__Title__c}"/>
                                                    <apex:column headerClass="slds-text-heading_label" headervalue="Phone" html-data-label="Phone" value="{!con.breadwinner_ns__Phone__c}"/>
                                                    <apex:column headerClass="slds-text-heading_label" headervalue="Email" html-data-label="Email" value="{!con.breadwinner_ns__Email__c}"/>
                                                </apex:dataTable>
                                            </apex:outputPanel>
                                            <apex:outputPanel layout="block" id="buttonsPanel" styleClass="slds-grid slds-wrap">
                                                <apex:outputPanel layout="block" styleClass="slds-col_padded slds-large-size_1-of-2 slds-medium-size_1-of-2 slds-size_1-of-1">
                                                    <div class="slds-form-element slds-m-top_small">
                                                        <label class="slds-form-element__label" >Records per Page</label>
                                                        <div class="slds-form-element__control">
                                                            <div class="slds-select_container slds-size_2-of-12">
                                                                <apex:selectList value="{!paginationSize}" multiselect="false" size="1" onchange="refreshPageSize();" styleClass="slds-select">
                                                                    <apex:selectOption itemLabel="10" itemValue="10"></apex:selectOption>
                                                                    <apex:selectOption itemLabel="20" itemValue="20"></apex:selectOption>
                                                                    <apex:selectOption itemLabel="30" itemValue="30"></apex:selectOption>
                                                                    <apex:selectOption itemLabel="40" itemValue="40"></apex:selectOption>
                                                                    <apex:selectOption itemLabel="50" itemValue="50"></apex:selectOption>
                                                                </apex:selectList>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </apex:outputPanel>
                                                <apex:outputPanel layout="block" styleClass="slds-col_padded slds-large-size_1-of-2 slds-medium-size_1-of-2 slds-size_1-of-1 slds-col_bump-left" style="text-align: right;">
                                                    <apex:commandButton reRender="contactTablePanel,buttonsPanel" value="First" action="{!setConContact.first}" disabled="{!!setConContact.hasPrevious}" title="First Page" styleClass="slds-button slds-button_brand slds-m-top_medium"/>
                                                    <apex:commandButton reRender="contactTablePanel,buttonsPanel" value="Previous" action="{!setConContact.previous}" disabled="{!!setConContact.hasPrevious}" title="Previous Page" styleClass="slds-button slds-button_brand slds-m-top_medium"/>
                                                    <apex:commandButton reRender="contactTablePanel,buttonsPanel" value="Next" action="{!setConContact.next}" disabled="{!!setConContact.hasNext}" title="Next Page" styleClass="slds-button slds-button_brand slds-m-top_medium"/>
                                                    <apex:commandButton reRender="contactTablePanel,buttonsPanel" value="Last" action="{!setConContact.last}" disabled="{!!setConContact.hasNext}" title="Last Page" styleClass="slds-button slds-button_brand slds-m-top_medium"/>
                                                </apex:outputPanel>
                                            </apex:outputPanel>
                                        </apex:outputPanel>
                                    </apex:outputPanel>
                                </apex:outputPanel>
                            </div>
                        </div>
                        <div class="slds-backdrop" id="contactModalBackdrop"></div>
                    </apex:outputPanel>
                    <!-- Contact Modal End -->
                    <div >
                        <div class="slds-modal slds-modal_xxx-small primaryContactError" aria-hidden="false" role="dialog" id="pcError_popup">
                            <div class="slds-modal__container">
                                <header class="slds-modal__header">
                                    <button id="pcError_popupCanBtn" class="slds-button slds-button_icon-inverse slds-modal__close" type="button" onclick="redirectToCustomerSelectionPage();">
                                        <svg aria-hidden="true" class="slds-button__icon slds-button__icon_large" >
                                            <use rel="noopener noreferrer" xlink:href="{!URLFOR($Asset.SLDS, 'assets/icons/action-sprite/svg/symbols.svg#close')}" ></use>
                                        </svg>
                                        <span class="slds-assistive-text">Close</span>
                                    </button>
                                    <h2 class="slds-text-heading_medium">{!IF(!isContactCreationFailed, 'Create Contact Failed', 'Attach Primary Contact Failed')}</h2>
                                </header>
                                <apex:outputPanel layout="block" styleClass="slds-modal__content slds-p-around_medium" id="pcErrorSection">
                                    <div>
                                        <apex:outputPanel id="customContactCreatonFailedMsg" rendered="{!isContactCreationFailed}">
                                            <div class="warningM3" style="border-radius: 4px" >
                                            Breadwinner was able to successfully create the NetSuite Customer {!createOrEditCompany.CompanyName__c}.<br/>However, the NetSuite Contact could not be created due to an error.<br/>
                                            The Error Message that was return from NetSuite was: <br/>
                                            {!primaryContactError}.<br/>
                                            You can either halt the process now (please keep in mind that the NetSuite Customer has just been created), or you can continue on and create the Sales Order.
                                            </div>
                                        </apex:outputPanel>
                                        <apex:outputPanel rendered="{!AND(!isContactCreationFailed, isPrimaryContactAttachFailed)}" html-data-id="primarycontact-pm">
                                            <apex:pageMessage severity="Warning" summary="{!primaryContactError}" id="primarycontact-pm"></apex:pageMessage>
                                        </apex:outputPanel>
                                    </div>
                                    <div align="right" class="slds-m-top_medium">
                                        <apex:commandLink rel="noopener noreferrer" id="pcError_SO_redirect" styleClass="slds-button slds-button_brand" value="Redirect to Created Sales Order" onclick="redirectAfterPrimaryContactError();" reRender="nothing" rendered="{!showCreateTransactionPanel}"/>
                                        <apex:commandLink rel="noopener noreferrer" id="pcError_Customer_redirect" styleClass="slds-button slds-button_brand" value="Continue Creating Sales Order" onclick="redirectAfterPrimaryContactError();" reRender="nothing" rendered="{!showCreateOrEditCompanyPanel}"/>
                                        <apex:commandLink rel="noopener noreferrer" id="Halt_the_Process" styleClass="slds-button slds-button_brand" value="Halt the Process" onclick="redirectToCustomerSelectionPage();" reRender="nothing" rendered="{!showCreateOrEditCompanyPanel}"/>
                                        <apex:actionFunction name="redirectToCustomerSelectionPage" status="action-status" action="{!goToNewTransactionPanel}" reRender="OverallPage,contactModalContent,pcErrorSection,estimateModalContent" oncomplete="showHideModal('pcError_popup','pcError_backdrop');setPageTitle();message();reRenderCheckIcons(['#stage1']);reRenderSalesOrderSVG();window.scrollTo(0,0);"/>
                                    </div>
                                </apex:outputPanel>
                            </div>
                        </div>
                        <div class="slds-backdrop" id="pcError_backdrop"></div>
                    </div>
					<!-- Estimate Modal Start -->
                    <apex:outputPanel layout="block" id="nsEstimateSelection">
                        <div class="slds-modal slds-modal_large" aria-hidden="false" role="dialog" id="estimateModal">
                            <div class="slds-modal__container" style="width:100%;padding:2rem;">
                                <div class="slds-modal__header">
                                    <button id="estimateModalCancelBtn" class="slds-button slds-button_icon-inverse slds-modal__close" type="button" onclick="showHideModal('estimateModal','estimateModalBackdrop');">
                                        <svg aria-hidden="true" class="slds-button__icon slds-button__icon_large" >
                                            <use rel="noopener noreferrer" xlink:href="{!URLFOR($Asset.SLDS, 'assets/icons/action-sprite/svg/symbols.svg#close')}" ></use>
                                        </svg>
                                        <span class="slds-assistive-text">Close</span>
                                    </button>
                                    <h2 class="slds-text-heading_medium">Select NetSuite Estimate</h2>
                                </div>
                                <apex:outputPanel layout="block" styleClass="slds-modal__content slds-p-around_medium" id="estimateModalContent">
                                    <apex:outputPanel layout="block" rendered="{!NOT(ISNULL(queryCustomerRelatedEstimates))}" >
										<apex:outputPanel layout="block" styleClass="slds-p-bottom_small slds-text-align_center" rendered="{!queryCustomerRelatedEstimates.size == 0}">
											<span><p>There are no NetSuite Estimates associated with this NetSuite Customer <b>{!selectedNSCompany.Name}</b></p></span>
										</apex:outputPanel>
                                        <apex:outputPanel layout="block" styleClass="slds-p-bottom_small" rendered="{!queryCustomerRelatedEstimates.size > 0}">
                                            <div class="estimateModalTitle">NetSuite Estimates related to <b>{!selectedNSCompany.Name}</b> Customer</div>
                                            <apex:outputPanel layout="block" styleClass="slds-p-around_small">
                                                <apex:dataTable value="{!queryCustomerRelatedEstimates}" var="estimate" id="estimatesTable" styleClass="slds-table slds-table_bordered slds-max-medium-table_stacked-horizontal">
                                                    <apex:column headerClass="slds-text-heading_label" headervalue="Estimate #" html-data-label="Estimate #">
                                                        <apex:commandlink rel="noopener noreferrer" value="{!estimate.Name}" onclick="setEstimate('{!JSENCODE(estimate.Name)}','{!JSENCODE(estimate.breadwinner_ns__InternalId__c)}')" reRender="nothing"/>
                                                    </apex:column>
                                                    <apex:column headerClass="slds-text-heading_label" headervalue="Status" html-data-label="Status" value="{!estimate.breadwinner_ns__Status__c}"/>
													<apex:column headerClass="slds-text-heading_label" headervalue="Probability (%)" html-data-label="Probability (%)" value="{!estimate.breadwinner_ns__Probability__c}"/>
													<apex:column headerClass="slds-text-heading_label" headervalue="Estimate Date" html-data-label="Estimate Date" value="{!estimate.breadwinner_ns__TranDate__c}"/>
													<apex:column headerClass="slds-text-heading_label" headervalue="Exp. Close Date" html-data-label="Exp. Close Date" value="{!estimate.breadwinner_ns__ExpectedCloseDate__c}"/>
                                                    <apex:column headerClass="slds-text-heading_label" headervalue="Total" html-data-label="Total" value="{!estimate.breadwinner_ns__Total__c}"/>
													<apex:column headerClass="slds-text-heading_label" headervalue="Currency" html-data-label="Currency" value="{!estimate.breadwinner_ns__CurrencyName__c}"/>
                                                </apex:dataTable>
                                            </apex:outputPanel>
                                        </apex:outputPanel>
                                    </apex:outputPanel>
                                </apex:outputPanel>
                            </div>
                        </div>
                        <div class="slds-backdrop" id="estimateModalBackdrop"></div>
                    </apex:outputPanel>
					<!-- Estimate Modal End -->
                </apex:form>
            </div>
        </body>
    </html>
</apex:page>