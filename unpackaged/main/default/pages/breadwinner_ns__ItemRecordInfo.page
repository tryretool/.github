                                                        <apex:outputPanel layout="block" styleClass="slds-col slds-large-size_1-of-3 slds-medium-size_1-of-2 slds-size_1-of-1 slds-large-order_1">
                                                            <div class="slds-form-element slds-border_bottom">
                                                                <label class="slds-form-element__label">Item Name</label>
                                                                <div class="slds-form-element__control">
                                                                    <span class="slds-form-element__static">
                                                                        <apex:outputField value="{!nsItem.breadwinner_ns__ItemId__c}" rendered="{!AND($ObjectType.breadwinner_ns__BW_Item__c.fields.breadwinner_ns__ItemId__c.Accessible,NOT(ISBLANK(nsItem.breadwinner_ns__ItemId__c)))}"/>
                                                                        <apex:outputField value="{!nsItem.Name}" rendered="{!NOT(AND($ObjectType.breadwinner_ns__BW_Item__c.fields.breadwinner_ns__ItemId__c.Accessible,NOT(ISBLANK(nsItem.breadwinner_ns__ItemId__c))))}"/>
                                                                    </span>
                                                                </div>
                                                            </div>
                                                        </apex:outputPanel>
                                                        <apex:outputPanel layout="block" styleClass="slds-col slds-large-size_1-of-3 slds-medium-size_1-of-2 slds-size_1-of-1 slds-large-order_2" rendered="{!$ObjectType.breadwinner_ns__BW_Item__c.fields.breadwinner_ns__Item_Type__c.Accessible}">
                                                            <div class="slds-form-element slds-border_bottom">
                                                                <label class="slds-form-element__label">Item Type</label>
                                                                <div class="slds-form-element__control">
                                                                    <span class="slds-form-element__static">
                                                                        <apex:outputText value="{!nsItem.breadwinner_ns__Item_Type__c}"/>
                                                                    </span>
                                                                </div>
                                                            </div>
                                                        </apex:outputPanel>
                                                        <apex:outputPanel layout="block" styleClass="slds-col slds-large-size_1-of-3 slds-medium-size_1-of-2 slds-size_1-of-1 slds-large-order_3" rendered="{!AND(NOT(contains(nsItem.breadwinner_ns__Item_Type__c,'Description')),NOT(contains(nsItem.breadwinner_ns__Item_Type__c,'Payment')),$ObjectType.breadwinner_ns__BW_Item__c.fields.breadwinner_ns__Parent__c.Accessible)}">
                                                            <div class="slds-form-element slds-border_bottom">
                                                                <label class="slds-form-element__label">Parent Item</label>
                                                                <div class="slds-form-element__control">
                                                                    <span class="slds-form-element__static">
                                                                        <apex:outputLink rel="noopener noreferrer" value="/{!URLFOR(nsItem.breadwinner_ns__Parent__c)}" rendered="{!NOT(ISNULL(nsItem.breadwinner_ns__Parent__c))}" target="_blank">
                                                                            <apex:outputText value="{!nsItem.Parent__r.Name}" rendered="{!ISNULL(nsItem.breadwinner_ns__Parent__c)}"/>
                                                                        </apex:outputLink>
                                                                        <apex:outputField value="{!nsItem.breadwinner_ns__Parent__c}"/>
                                                                    </span>
                                                                </div>
                                                            </div>
                                                        </apex:outputPanel>
                                                        <apex:outputPanel layout="block" styleClass="slds-col slds-large-size_1-of-3 slds-medium-size_1-of-2 slds-size_1-of-1 slds-large-order_4" rendered="{!AND(NOT(contains(nsItem.breadwinner_ns__Item_Type__c,'Description')),$ObjectType.breadwinner_ns__BW_Item__c.fields.breadwinner_ns__DisplayName__c.Accessible)}">
