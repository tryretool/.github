                                                                        <apex:outputField value="{!nsItem.breadwinner_ns__Description__c}"/>
                                                                    </span>
                                                                </div>
                                                            </div>
                                                        </apex:outputPanel>
                                                        <apex:outputPanel layout="block" styleClass="slds-col slds-large-size_1-of-3 slds-medium-size_1-of-2 slds-size_1-of-1 slds-large-order_8" rendered="{!AND(NOT(OR(contains(nsItem.breadwinner_ns__Item_Type__c,'Description'),contains(nsItem.breadwinner_ns__Item_Type__c,'Payment'),contains(nsItem.breadwinner_ns__Item_Type__c,'Subtotal'))),$ObjectType.breadwinner_ns__BW_Item__c.fields.breadwinner_ns__UpcCode__c.Accessible)}">
                                                            <div class="slds-form-element slds-border_bottom">
                                                                <label class="slds-form-element__label">UPC Code</label>
                                                                <div class="slds-form-element__control">
                                                                    <span class="slds-form-element__static">
                                                                        <apex:outputField value="{!nsItem.breadwinner_ns__UpcCode__c}"/>
                                                                    </span>
                                                                </div>
                                                            </div>
                                                        </apex:outputPanel>
                                                        <apex:outputPanel layout="block" styleClass="slds-col slds-large-size_1-of-3 slds-medium-size_1-of-2 slds-size_1-of-1 slds-large-order_9" rendered="{!AND($ObjectType.breadwinner_ns__BW_Item__c.fields.breadwinner_ns__NetSuite_Org_Id__c.Accessible,$ObjectType.breadwinner_ns__BW_Item__c.fields.breadwinner_ns__InternalId__c.Accessible,NOT(ISBLANK(netSuite_URL)))}">
                                                            <div class="slds-form-element slds-border_bottom">
                                                                <label class="slds-form-element__label">View this Item in NetSuite</label>
                                                                <div class="slds-form-element__control">
                                                                    <span class="slds-form-element__static">
                                                                        <apex:outputLink rel="noopener noreferrer" value="{!netSuite_URL}" target="_blank">Click Here</apex:outputLink>
