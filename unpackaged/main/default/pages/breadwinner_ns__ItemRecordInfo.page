                                                                <label class="slds-form-element__label">Display Name</label>
                                                                <div class="slds-form-element__control">
                                                                    <span class="slds-form-element__static">
                                                                        <apex:outputField value="{!nsItem.breadwinner_ns__DisplayName__c}"/>
                                                                    </span>
                                                                </div>
                                                            </div>
                                                        </apex:outputPanel>
                                                        <apex:outputPanel layout="block" styleClass="slds-col slds-large-size_1-of-3 slds-medium-size_1-of-2 slds-size_1-of-1 slds-large-order_5" rendered="{!AND(NOT(contains(nsItem.breadwinner_ns__Item_Type__c,'Description')),NOT(contains(nsItem.breadwinner_ns__Item_Type__c,'Discount')),NOT(contains(nsItem.breadwinner_ns__Item_Type__c,'Payment')),$ObjectType.breadwinner_ns__BW_Item__c.fields.breadwinner_ns__UnitsType__c.Accessible)}">
                                                            <div class="slds-form-element slds-border_bottom">
                                                                <label class="slds-form-element__label">Units Type</label>
                                                                <div class="slds-form-element__control">
                                                                    <span class="slds-form-element__static">
                                                                        <apex:outputField value="{!nsItem.breadwinner_ns__UnitsType__c}"/>
                                                                    </span>
                                                                </div>
                                                            </div>
                                                        </apex:outputPanel>
                                                        <apex:outputPanel layout="block" styleClass="slds-col slds-large-size_1-of-3 slds-medium-size_1-of-2 slds-size_1-of-1 slds-large-order_6" rendered="{!$ObjectType.breadwinner_ns__BW_Item__c.fields.breadwinner_ns__IsInactive__c.Accessible}">
