                                                                        <apex:outputLink rel="noopener noreferrer" value="/{!URLFOR(matrix.id)}" target="_blank"><apex:outputText value="{!matrix.Name}"/></apex:outputLink>
                                                                    </apex:column>
                                                                    <apex:column headerValue="Type" html-data-label="Type" styleClass="slds-cell-wrap" value="{!matrix.breadwinner_ns__Item_Type__c}" rendered="{!$ObjectType.breadwinner_ns__BW_Item__c.fields.breadwinner_ns__Item_Type__c.Accessible}"/>
                                                                    <apex:column headerValue="Description" html-data-label="Description" styleClass="slds-cell-wrap" rendered="{!AND($ObjectType.breadwinner_ns__BW_Item__c.fields.breadwinner_ns__Item_Type__c.Accessible, IF(contains(matrix.breadwinner_ns__Item_Type__c,'Purchase'),$ObjectType.breadwinner_ns__BW_Item__c.fields.breadwinner_ns__PurchaseDescription__c.Accessible,$ObjectType.breadwinner_ns__BW_Item__c.fields.breadwinner_ns__SalesDescription__c.Accessible))}">
                                                                        <apex:outputText value="{!IF(contains(matrix.breadwinner_ns__Item_Type__c,'Purchase'),matrix.breadwinner_ns__PurchaseDescription__c,matrix.breadwinner_ns__SalesDescription__c)}"/>
                                                                    </apex:column>
                                                                    <apex:column headerValue="Units Type" html-data-label="Units Type" styleClass="slds-cell-wrap" value="{!matrix.breadwinner_ns__UnitsType__c}" rendered="{!$ObjectType.breadwinner_ns__BW_Item__c.fields.breadwinner_ns__UnitsType__c.Accessible}"/>
                                                                </apex:DataTable>
                                                            </apex:outputPanel>
                                                            <apex:outputPanel layout="block" rendered="{!nsMatrixItems.size==0}">
                                                                <div class="slds-has-divider_bottom slds-has-divider_top slds-m-top_x-small slds-p-vertical_small">
                                                                    <span><center>No Matrix items have been created for this Item</center></span>
                                                                </div>
                                                            </apex:outputPanel>
                                                        </div>
                                                    </div>
                                                </apex:outputPanel>
                                            </div>
                                        </div>
                                        <script>
                                        function selectItemInfoTab(elmnt,currencyName) {
                                            // slds-active
                                            j$(elmnt).parent(".liTab").siblings(".slds-is-active").removeClass("slds-is-active");
                                            j$(elmnt).parent(".liTab").addClass("slds-is-active");  
                                            
                                            // tabindex
                                            j$(elmnt).parent(".liTab").siblings().children("a").attr("tabindex", -1);
                                            j$(elmnt).attr("tabindex", 0);
                                            
                                            // aria-selected
                                            j$(elmnt).parent(".liTab").siblings().children("a").attr("aria-selected", false);
                                            j$(elmnt).attr("aria-selected", true);
                                            if(currencyName!='NULL'){
                                                document.getElementById('{!JSENCODE($Component.theForm.currency)}').value = currencyName;
                                                refreshPrices();
                                            }
                                            else{
                                                // hiding previouly selected tab (slds-show/slds-hide)
                                                j$(elmnt).closest(".slds-tabs_default").children("div[role='tabpanel'].slds-show").addClass("slds-hide").removeClass("slds-show");
                                                // displaying newly selected tab (slds-show/slds-hide)
                                                j$(elmnt).closest(".slds-tabs_default").children("div[aria-labelledby='"+elmnt.id+"']").addClass("slds-show").removeClass("slds-hide");
                                            }    
                                        }
                                        </script>       
                                    </apex:outputPanel>
                                    <apex:outputPanel layout="block" styleClass="slds-card slds-m-bottom_small sections" rendered="{!AND($ObjectType.breadwinner_ns__BW_Item__c.fields.breadwinner_ns__Do_Not_Match__c.Accessible,NOT(nsItem.breadwinner_ns__Do_Not_Match__c))}">
                              		<div class="slds-m-vertical_none slds-p-top_none slds-border_none">
                                    <c:customRelatedListSingle cmpUniqueId="product"
                                             relatedListTitle="Associated Salesforce Products"                                    
                                             parentRecordId="{!$CurrentPage.parameters.Id}"
                                             relatedChildObjectAPIName="Product2"
                                             relatedLookupFieldAPIName="NetSuite_Item__c"
                                             relatedListIconName="standard:product_item"
                                             customMessageOnNoData="This NetSuite Item is yet to be matched with a Salesforce Product"
                                    >
                                    </c:customRelatedListSingle>
                                    </div>
                                    </apex:outputPanel>
                            </apex:outputPanel>
                            </apex:outputPanel>
                        </apex:outputPanel>
                        <apex:outputPanel layout="block" id="ItemControlSyncSection" rendered="{!AND(isControlSync,NOT(showBWLayout))}" styleClass="{!IF(isLightning,'slds-p-around_small slds-brand-band slds-brand-band_cover slds-brand-band_medium','')}">
                            <div class="slds-page-header slds-m-bottom_small" role="banner">
                                <div class="slds-media slds-media_center">
                                    <div class="slds-media__figure">
                                        <span class="slds-icon_container slds-icon-custom-custom3 Icon">
                                            <svg class="slds-icon slds-page-header__icon" aria-hidden="true">
                                                <use xmlns:xlink="http://www.w3.org/1999/xlink" rel="noopener noreferrer" xlink:href="{!URLFOR($Asset.SLDS, 'assets/icons/custom-sprite/svg/symbols.svg#custom3')}" />
                                            </svg>
                                            <span class="slds-assistive-text">NetSuite Item Control Sync</span>
                                        </span>
                                    </div>
                                    <div class="slds-media__body">
                                        <p class="slds-page-header__title slds-truncate slds-align-middle" title="Breadwinner NetSuite Item">NetSuite Item Control Sync</p>
                                        <p class="slds-text-body_small slds-line-height_reset">Breadwinner  NetSuite</p>
                                    </div>
                                </div>
                            </div>
                            <apex:pageMessages escape="false" id="pm"/>
                            <apex:outputPanel styleClass="OrgError">
                                <apex:pageMessage escape="false" severity="ERROR" rendered="{!ISNULL(appConfig)}" summary="The NetSuite Org {!netSuiteItem.breadwinner_ns__NetSuite_Org_Id__c} related to this NetSuite Item is Not connected to Breadwinner. Please connect it from Breadwinner Tab."/>
                            </apex:outputPanel>
                            <apex:actionFunction name="changeAssociatedSFProduct" status="action-status" reRender="controlstatus,pm" oncomplete="addallstyles();"/>
                            <apex:outputPanel id="controlstatus"> 
                                <apex:outputPanel layout="block" styleClass="slds-card">
                                <apex:outputPanel >
                                        <div class="slds-panel__section slds-p-bottom_none">
                                            <div class="slds-section slds-is-open slds-m-vertical_none">
                                                <h3 class="slds-section__title slds-theme_shade">
                                                    <span class="slds-truncate slds-p-horizontal_small">Breadwinner Sync Status</span>
                                                </h3>
                                                <apex:outputPanel styleClass="slds-section__content slds-grid slds-wrap slds-grid_vertical slds-p-horizontal_small slds-m-top_xx-small">
                                                    <apex:outputPanel layout="block" styleClass="slds-col slds-large-size_1-of-1 slds-medium-size_1-of-1 slds-grid slds-wrap" rendered="{!AND($ObjectType.breadwinner_ns__BW_Item__c.fields.breadwinner_ns__Do_not_Import_Inventory_Locations__c.Accessible,$ObjectType.breadwinner_ns__BW_Item__c.fields.breadwinner_ns__Do_not_Import_Inventory_Locations__c.Updateable)}">
                                                        <div class="slds-col slds-text-heading_small slds-m-bottom_none slds-p-right_medium slds-p-top_xxx-small slds-large-size_2-of-4 slds-medium-size_2-of-4">Do you want Breadwinner to sync Inventory Locations for this Item?</div>
                                                        <div class="slds-col slds-form-element slds-large-size_1-of-4 slds-medium-size_1-of-4">   
                                                            <label class="slds-checkbox_toggle slds-grid"> 
                                                                <apex:inputCheckbox id="SyncInventoryLocations" value="{!syncInventoryLocations}"/>
                                                                <span  class="slds-checkbox_faux_container" aria-live="assertive">
                                                                    <span class="slds-checkbox_faux"></span>
                                                                    <span class="slds-checkbox_on">Yes</span>
                                                                    <span class="slds-checkbox_off">No</span>
                                                                </span>
                                                            </label>
                                                        </div>
                                                    </apex:outputPanel>
                                                 </apex:outputPanel>
                                            </div>
                                        </div>
                                    </apex:outputPanel>
                                    <div class="slds-panel__section slds-p-bottom_none">
                                        <div class="slds-section slds-is-open slds-m-vertical_none">
                                            <h3 class="slds-section__title slds-theme_shade">
                                                <span class="slds-truncate slds-p-horizontal_small">Match to Salesforce Product</span>
                                            </h3>
                                            <apex:outputPanel styleClass="slds-section__content slds-grid slds-wrap slds-grid_vertical slds-p-horizontal_small slds-m-top_xx-small">
                                                <div class="slds-col slds-text-heading_small slds-large-size_1-of-1">This NetSuite Item 
                                                    <b>
                                                        <apex:outputText value="{!netSuiteItem.breadwinner_ns__ItemId__c}" rendered="{!AND($ObjectType.breadwinner_ns__BW_Item__c.fields.breadwinner_ns__ItemId__c.Accessible,NOT(ISBLANK(nsItem.breadwinner_ns__ItemId__c)))}"/>
                                                        <apex:outputText value="{!netSuiteItem.Name}" rendered="{!NOT(AND($ObjectType.breadwinner_ns__BW_Item__c.fields.breadwinner_ns__ItemId__c.Accessible,NOT(ISBLANK(nsItem.breadwinner_ns__ItemId__c))))}"/>
                                                    </b>
                                                    <apex:outputText rendered="{!OR(AND($ObjectType.breadwinner_ns__BW_Item__c.fields.breadwinner_ns__Do_Not_Match__c.Accessible,netSuiteItem.breadwinner_ns__Do_Not_Match__c),AND($ObjectType.Product2.fields.breadwinner_ns__NetSuite_Item__c.Accessible,NOT(AssociatedProduct.size>0)))}"> is currently not matched with any Salesforce Product.</apex:outputText>
                                                    <apex:outputText rendered="{!AND($ObjectType.breadwinner_ns__BW_Item__c.fields.breadwinner_ns__Do_Not_Match__c.Accessible,$ObjectType.Product2.fields.breadwinner_ns__NetSuite_Item__c.Accessible,NOT(netSuiteItem.breadwinner_ns__Do_Not_Match__c),(AssociatedProduct.size>0))}"> is currently matched with the Salesforce Product <b>{!AssociatedProduct[0].Name}</b>.
                                                    </apex:outputText>
                                                </div>
                                                <apex:outputPanel layout="block" styleClass="slds-col slds-large-size_1-of-1 slds-medium-size_1-of-1 slds-p-top_medium slds-grid slds-wrap" rendered="{!AND($ObjectType.breadwinner_ns__BW_Item__c.fields.breadwinner_ns__Do_Not_Match__c.Accessible,$ObjectType.Product2.fields.breadwinner_ns__NetSuite_Item__c.Accessible,$ObjectType.breadwinner_ns__BW_Item__c.fields.breadwinner_ns__Do_Not_Match__c.Updateable,$ObjectType.Product2.fields.breadwinner_ns__NetSuite_Item__c.Updateable)}">
                                                    <div class="slds-text-heading_small slds-m-bottom_none slds-p-right_medium slds-p-top_xxx-small slds-large-size_1-of-1 slds-medium-size_1-of-1 slds-grid">
                                                        <div class="slds-text-heading_small slds-m-bottom_none slds-p-right_medium slds-p-top_xxx-small">Do you want this NetSuite Item to be matched with a Salesforce Product?</div>
                                                        <div class="slds-form-element">   
                                                            <label class="slds-checkbox_toggle slds-grid"> 
                                                                <apex:inputCheckbox id="DoNotAssscociate" value="{!hasAssociatedSFProduct}" onclick="changeAssociatedSFProduct();"/>
                                                                <span  class="slds-checkbox_faux_container" aria-live="assertive">
                                                                    <span class="slds-checkbox_faux"></span>
                                                                    <span class="slds-checkbox_on">Match</span>
                                                                    <span class="slds-checkbox_off">Do not match</span>
                                                                </span>
                                                            </label>
                                                        </div>
                                                    </div>
                                                    <apex:outputPanel layout="block" id="sfItemLookupPanel">
                                                        <apex:outputPanel styleClass="slds-grid" layout="block" rendered="{!hasAssociatedSFProduct}">
                                                            <label class="slds-text-heading_small slds-p-top_xx-small" for="chosenSFUser">Choose Salesforce Product</label>
                                                            <div class="slds-form-element__control slds-p-left_small">
                                                                <apex:inputField id="chosenSFUser" styleClass="slds-input accountInput slds-large-size_1-of-1" value="{!AssociatedProductLookUp.Product2Id}" style="width:250px;"/>
                                                            </div>
                                                        </apex:outputPanel>
                                                    </apex:outputPanel>
                                                </apex:outputPanel>
                                            </apex:outputPanel>
                                        </div>
                                        <apex:outputPanel id="panelbuttons" layout="block" styleClass="slds-m-top_small slds-m-bottom_medium slds-text-align_center">
											<apex:commandButton value="Cancel" styleClass="slds-button slds-button_neutral" action="{!Cancel}" status="action-status" id="cancelbutton"/>
                                            <apex:commandButton value="Save" styleClass="slds-button slds-button_brand slds-m-left_x-small" id="savebutton" status="action-status" action="{!changeItemSyncStatus}" oncomplete="addallstyles();message();" disabled="{!OR(NOT($ObjectType.breadwinner_ns__BW_Item__c.Updateable),ISNULL(appConfig),NOT($ObjectType.Product2.Updateable))}" reRender="pm,controlstatus"/>
                                        </apex:outputPanel>
                                    </div>
                                </apex:outputPanel>
                            </apex:outputPanel>    
                        </apex:outputPanel>
                    </apex:outputPanel>
                </apex:form>
            </div>
        </body>
    </html>
</apex:page>