												attributes,
												"CreateCompanyComponentid",
												(cmp) => {
													const spinner = document.getElementById('CreateCompanyComponentid-spin');
													if (spinner) spinner.style.display = "none";
													console.log('LWC component created successfully');
												}
											);
										});
