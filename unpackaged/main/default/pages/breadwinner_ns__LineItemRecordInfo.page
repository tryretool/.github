        <body style="{!IF(AND(isLightning,showBWLayout), 'background: rgba(176, 196, 223, 1);','')}">
            <div class="slds-scope">
                <apex:form id="theForm">
                    <apex:variable var="txnNameApexVar" value="{!CASE(lineItem.breadwinner_ns__Transaction_Type__c,'SalesOrder','Sales Order','CreditMemo','Credit Memo','VendorCredit','Vendor Credit','CashSale','Cash Sale','CashRefund','Cash Refund','ReturnAuthorization','Return Authorization (RMA)','PurchaseOrder','Purchase Order','ItemFulfillment','Item Fulfillment','VendorBill','Vendor Bill','VendorPayment','Vendor Payment','VendorCredit','Vendor Credit',lineItem.breadwinner_ns__Transaction_Type__c)}"/>
                    <apex:outputPanel rendered="{!ISNULL(recordId)}" layout="block">
                        <apex:pageMessage severity="ERROR" strength="2" summary="Breadwinner does not support NetSuite Line Item creation from within Salesforce." escape="false" />
                    </apex:outputPanel>
                    <apex:outputPanel layout="block" rendered="{!NOT(showBWLayout)}">
                        <div class="slds-box slds-align_absolute-center" style="height: 200px;">
                            <table width="100%" border="0" cellpadding="0" cellspacing="0">
                                <tr>
                                    <td class="image" style="width: 200px;">
                                        <apex:image url="{!URLFOR($Resource.breadwinner_ns__Breadwinner_for_NetSuite, 'Breadwinner/Images/breadwinner.png')}" />
                                    </td>
                                    <td class="content">
                                        <div class="slds-text-align_center" style="font-size: 2.1em;">
                                            <a id="LineItemLayout" rel="noopener noreferrer" href="{!$Page.LineItemRecordInfo}?id={!recordId}&showBWLayout=true" target="_parent" >View NetSuite Line Item in Dedicated Layout</a>
                                        </div>
                                    </td>
                                </tr>
                            </table>
                        </div>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!AND(showBWLayout,ISNULL(lineItem.id),NOT(ISNULL(recordId)))}" layout="block" styleClass="{!IF(isLightning,'slds-p-around_small','')}">
                        <apex:pageMessage severity="ERROR" strength="2" summary="<b>Record deleted<b/>" escape="false" rendered="{!showRecordDeletedMessage}">
                            <p>The record you attempted to access has been deleted. The user who deleted this record may be able to recover it from the Recycle Bin. Deleted data is stored in the Recycle Bin for 15 days.</p>
                        </apex:pageMessage>
                        <apex:pageMessages id="beforeRecordFetch" rendered="{!!showRecordDeletedMessage}"/>
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!AND(showBWLayout,NOT(ISNULL(lineItem.id)))}" layout="block" styleClass="{!IF(isLightning,'slds-p-around_small slds-brand-band slds-brand-band_cover slds-brand-band_medium','')}">
                        <apex:outputPanel layout="block">
                            <!-- Page Header Start -->
                            <div class="slds-page-header slds-m-bottom_small" role="banner">
                                <div class="slds-grid">
                                    <div class="slds-col slds-has-flexi-truncate">
                                        <div class="slds-media slds-media_center">
                                            <div class="slds-media__figure">
                                                <span class="slds-icon_container slds-icon-custom-custom93">
                                                    <svg class="slds-icon slds-page-header__icon" aria-hidden="true">
                                                        <use rel="noopener noreferrer" xlink:href="{!URLFOR($Asset.SLDS, 'assets/icons/custom-sprite/svg/symbols.svg#custom93')}"></use>
                                                    </svg>
                                                    <span class="slds-assistive-text"></span>
                                                </span>
                                            </div>
                                            <div class="slds-media__body">
                                                <p>NetSuite {!IF(AND($ObjectType.BW_Line_Item__c.fields.Line_Item_Type__c.Accessible,lineItem.Line_Item_Type__c == 'Expense'), 'Expense', If(lineItem.Line_Item_Type__c == 'Time', 'Billable Time', 'Line Item'))}</p>
                                                <p class="slds-page-header__title slds-truncate"><apex:outputText value="{!lineItem.Name}" rendered="{!$ObjectType.breadwinner_ns__BW_Line_Item__c.fields.Name.Accessible}"/>&nbsp;
                                                    <apex:outputText value="{!lineItem.ParentEntity__r.breadwinner_ns__CompanyName__c}" rendered="{!IF(AND($ObjectType.breadwinner_ns__BW_Company__c.fields.breadwinner_ns__CompanyName__c.Accessible,NOT(ISNULL(lineItem.breadwinner_ns__ParentEntity__c))),'TRUE','FALSE')}" styleClass="grayText"/>
                                                    <apex:outputText value="{!lineItem.ParentEntity__r.Name}" rendered="{!AND($ObjectType.breadwinner_ns__BW_Company__c.fields.Name.Accessible,$ObjectType.breadwinner_ns__BW_Company__c.fields.breadwinner_ns__CompanyName__c.Accessible,$ObjectType.breadwinner_ns__BW_Company__c.fields.breadwinner_ns__FirstName__c.Accessible,AND(ISNULL(lineItem.ParentEntity__r.breadwinner_ns__CompanyName__c),ISNULL(lineItem.ParentEntity__r.breadwinner_ns__FirstName__c)))}" styleClass="grayText"/>
                                                    <apex:outputPanel rendered="{!IF(AND($ObjectType.breadwinner_ns__BW_Company__c.fields.breadwinner_ns__IsPerson__c.Accessible,$ObjectType.breadwinner_ns__BW_Company__c.fields.breadwinner_ns__CompanyName__c.Accessible,lineItem.ParentEntity__r.breadwinner_ns__IsPerson__c,ISNULL(lineItem.ParentEntity__r.breadwinner_ns__CompanyName__c)),'TRUE','FALSE')}" styleClass="grayText">
                                                        <apex:outputText value="{!lineItem.ParentEntity__r.breadwinner_ns__Salutation__c} " rendered="{!$ObjectType.breadwinner_ns__BW_Company__c.fields.breadwinner_ns__Salutation__c.Accessible}"/>
                                                        <apex:outputText value="{!lineItem.ParentEntity__r.breadwinner_ns__FirstName__c} " rendered="{!$ObjectType.breadwinner_ns__BW_Company__c.fields.breadwinner_ns__FirstName__c.Accessible}"/>
                                                        <apex:outputText value="{!lineItem.ParentEntity__r.breadwinner_ns__MiddleName__c} " rendered="{!$ObjectType.breadwinner_ns__BW_Company__c.fields.breadwinner_ns__MiddleName__c.Accessible}"/>
                                                        <apex:outputText value="{!lineItem.ParentEntity__r.breadwinner_ns__LastName__c} " rendered="{!$ObjectType.breadwinner_ns__BW_Company__c.fields.breadwinner_ns__LastName__c.Accessible}"/>
                                                    </apex:outputPanel>&nbsp;
                                                    <apex:outputText rendered="{!IF(lineItem.breadwinner_ns__Transaction_Type__c == 'Estimate',$ObjectType.breadwinner_ns__BW_Line_Item__c.fields.breadwinner_ns__Estimate__c.Accessible,IF(lineItem.breadwinner_ns__Transaction_Type__c == 'SalesOrder',$ObjectType.breadwinner_ns__BW_Line_Item__c.fields.breadwinner_ns__Sales_Order__c.Accessible,IF(OR(lineItem.breadwinner_ns__Transaction_Type__c =='Invoice',lineItem.breadwinner_ns__Transaction_Type__c =='CashSale'),$ObjectType.breadwinner_ns__BW_Line_Item__c.fields.breadwinner_ns__Invoice__c.Accessible,IF(OR(lineItem.breadwinner_ns__Transaction_Type__c =='CreditMemo',lineItem.breadwinner_ns__Transaction_Type__c =='CashRefund'),$ObjectType.breadwinner_ns__BW_Line_Item__c.fields.breadwinner_ns__Credit_Memo__c.Accessible,IF(lineItem.breadwinner_ns__Transaction_Type__c =='ItemFulfillment',$ObjectType.breadwinner_ns__BW_Line_Item__c.fields.breadwinner_ns__Item_Fulfillment__c.Accessible,$ObjectType.breadwinner_ns__BW_Line_Item__c.fields.breadwinner_ns__Return_Authorization__c.Accessible)))))}" value="{!CASE(lineItem.breadwinner_ns__Transaction_Type__c, 'Estimate',lineItem.Estimate__r.Name,'SalesOrder',lineItem.Sales_Order__r.Name,'Invoice',lineItem.Invoice__r.Name,'CashSale',lineItem.Invoice__r.Name,'CreditMemo',lineItem.Credit_Memo__r.Name,'CashRefund',lineItem.Credit_Memo__r.Name,'ReturnAuthorization',lineItem.Return_Authorization__r.Name,lineItem.Item_Fulfillment__r.Name)}" styleClass="grayText"/>
