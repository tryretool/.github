                                                            <apex:outputLink rel="noopener noreferrer" value="/{!URLFOR(lineItem.breadwinner_ns__Sales_Order__c)}" rendered="{!IF(NOT(ISNULL(lineItem.breadwinner_ns__Sales_Order__c)),'TRUE','FALSE')}" target="_blank" id="AssociatedSalesOrder">{!lineItem.Sales_Order__r.Name}</apex:outputLink>
