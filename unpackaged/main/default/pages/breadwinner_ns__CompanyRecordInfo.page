                                    <div class="slds-spinner__dot-a"></div>
                                    <div class="slds-spinner__dot-b"></div>
                                </div>
                            </div>
                            </div>
                            <script>
                                $Lightning.use("{!getCompNamespacePrefix}:lightningOutUnstyled", function() {
                                    $Lightning.createComponent("{!getCompNamespacePrefix}:netsuiteCompany",
                                    { 
                                        lookupId : '{!lookupId}',
                                        currentPage : '{!$page.CompanyRecordInfo}'
                                    },
                                    "CreateCompanyComponentid", 
                                    function(cmp) {
                                        document.getElementById('CreateCompanyComponentid-spin').style.display="none";
                                    }); 
                                });
                            </script>
                        </apex:outputPanel>  
                    </apex:outputPanel>
                    <apex:outputPanel id="CustomerPage" layout="block" rendered="{!NOT(ISNULL(recordId))}">
                        <apex:actionStatus id="action-status" layout="block">
