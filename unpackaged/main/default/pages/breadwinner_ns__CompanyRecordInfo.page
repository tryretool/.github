        <body style="{!HTMLENCODE(IF(AND(isLightning,OR(showBWLayout,isControlSync)), 'background: rgba(176, 196, 223, 1);',''))}">
            <div class="slds-scope slds-is-relative">
                <apex:form id="formId">
                    <apex:outputPanel rendered="{!ISNULL(recordId)}" layout="block">
                        <apex:outputPanel layout="block" styleClass="slds-scope slds-p-around_x-small">
