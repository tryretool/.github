<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Owned_Days__c</fullName>
    <description>The number of days the Account has been owned by the current owner.</description>
    <externalId>false</externalId>
    <formula>IF(ISBLANK( Owner_Assigned_At__c ),0,NOW()-Owner_Assigned_At__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>The number of days the Account has been owned by the current owner.</inlineHelpText>
    <label>Owned Days</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <securityClassification>Safe</securityClassification>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
