<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Is_First_Snapshot_of_Month__c</fullName>
    <formula>IF(DAY(Snapshot_Date__c) &lt;= 7,TRUE, FALSE)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>True if the weekly snapshot is the first taken in the calendar month</inlineHelpText>
    <label>Is First Snapshot of Month?</label>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
