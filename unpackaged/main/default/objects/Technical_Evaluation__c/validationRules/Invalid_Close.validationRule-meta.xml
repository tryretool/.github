<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Invalid_Close</fullName>
    <active>true</active>
    <errorConditionFormula>ISBLANK(Close_Method__c) &amp;&amp;
NOT(ISCHANGED(Permission_Check__c)) &amp;&amp;
Is_Closed__c &amp;&amp;
ISCHANGED(Is_Closed__c)</errorConditionFormula>
    <errorMessage>You must use the Salesforce UI to close the technical evaluation</errorMessage>
</ValidationRule>
