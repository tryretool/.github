<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>End_date_last_of_month</fullName>
    <active>true</active>
    <errorConditionFormula>DAY(End_Date__c + 1) != 1</errorConditionFormula>
    <errorDisplayField>End_Date__c</errorDisplayField>
    <errorMessage>End date must be on the last date of a month</errorMessage>
</ValidationRule>
