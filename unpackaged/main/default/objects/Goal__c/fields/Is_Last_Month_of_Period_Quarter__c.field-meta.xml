<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Is_Last_Month_of_Period_Quarter__c</fullName>
    <formula>IF(CASE(MONTH(Period_Start__c),
 3,1,
 6,1,
 9,1,
 12,1,
 0
)=1,TRUE,FALSE)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>True if the period start/end date represent a month that&#39;s the last month of the quarter</inlineHelpText>
    <label>Is Last Month of Period Quarter?</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
