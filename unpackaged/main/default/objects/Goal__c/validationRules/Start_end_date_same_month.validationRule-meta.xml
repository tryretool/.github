<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Start_end_date_same_month</fullName>
    <active>true</active>
    <errorConditionFormula>MONTH(Start_Date__c) != MONTH(End_Date__c) || YEAR(Start_Date__c) != YEAR(End_Date__c)</errorConditionFormula>
    <errorMessage>Start and end dates must be in the same month and year</errorMessage>
</ValidationRule>
