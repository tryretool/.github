    <description>Changed {!target_user_goal.New_Annualized_Rev_Attainment_QTD__c} to {!target_user_goal.New_Annualized_Rev_Attainment_CQ__c} in slack post.</description>
    <environments>Default</environments>
    <formulas>
        <name>build_renewal_row</name>
        <dataType>String</dataType>
        <expression>&quot;• &lt;https://retool.lightning.force.com/&quot;&amp;{!current_open_renewal.Id}&amp;&quot;|&quot;&amp;{!current_open_renewal.Name}&amp;&quot;&gt; renewing &quot;&amp;IF({!current_open_renewal.Auto_close__c},&quot;*automatically* &quot;,&quot;&quot;)&amp;&quot;in &quot;&amp;TEXT({!current_open_renewal.Time_to_Renewal_days__c})&amp;&quot; days&quot;
&amp;IF({!current_open_renewal.Time_to_Renewal_days__c}&lt;0,&quot; :exclamation: _LATE RENEWAL_ :exclamation:&quot;,NULL)
&amp;BR()</expression>
    </formulas>
    <formulas>
        <name>calc_attainment</name>
        <dataType>Number</dataType>
        <expression>{!target_user_goal.New_Annualized_Revenue_Attainment_CQ__c}*100</expression>
        <scale>1</scale>
    </formulas>
    <formulas>
        <name>call_submit_due_time</name>
        <dataType>String</dataType>
        <expression>CASE({!notification_setting.Call_Submit_Deadline_Hour__c}+{!target_user_goal.Assignee__r.Time_Zone_UTC_Offset__c},
0,&quot;midnight&quot;,
1,&quot;1am&quot;,
2,&quot;2am&quot;,
3,&quot;3am&quot;,
4,&quot;4am&quot;,
5,&quot;5am&quot;,
6,&quot;6am&quot;,
7,&quot;7am&quot;,
8,&quot;8am&quot;,
9,&quot;9am&quot;,
10,&quot;10am&quot;,
11,&quot;11am&quot;,
12,&quot;noon&quot;,
13,&quot;1pm&quot;,
14,&quot;2pm&quot;,
15,&quot;3pm&quot;,
16,&quot;4pm&quot;,
17,&quot;5pm&quot;,
18,&quot;6pm&quot;,
19,&quot;7pm&quot;,
20,&quot;8pm&quot;,
21,&quot;9pm&quot;,
22,&quot;10pm&quot;,
23,&quot;11pm&quot;,
&quot;Undefined&quot;
)</expression>
    </formulas>
    <formulas>
        <name>close_date_post</name>
        <dataType>String</dataType>
        <expression>IF({!opp_cnt_close_date_in_past}&gt;0,&quot;• You have &quot;&amp;TEXT({!opp_cnt_close_date_in_past})&amp;&quot; open opportunities with a close date that is in the past. &lt;https://retool.lightning.force.com/lightning/r/Report/00ODu000000L3NxMAK/view?fv0=&quot;&amp;{!target_user_goal.Assignee__c}&amp;&quot;|Please update the close date on these opportunities&gt;.&quot;&amp;BR(),NULL)</expression>
    </formulas>
    <formulas>
        <name>current_month_date</name>
        <dataType>Date</dataType>
        <expression>DATE(YEAR({!$Flow.CurrentDate}),MONTH({!$Flow.CurrentDate}),1)</expression>
    </formulas>
    <formulas>
        <name>current_month_name</name>
        <dataType>String</dataType>
        <expression>CASE(MONTH({!$Flow.CurrentDate}),
1, &quot;January&quot;,
2, &quot;February&quot;,
3, &quot;March&quot;, 
4, &quot;April&quot;, 
5, &quot;May&quot;, 
6, &quot;June&quot;,
7, &quot;July&quot;,
8, &quot;August&quot;,
9, &quot;September&quot;,
10, &quot;October&quot;,
11, &quot;November&quot;,
12, &quot;December&quot;,
&quot;None&quot;)</expression>
    </formulas>
    <formulas>
        <name>current_quarter</name>
        <dataType>String</dataType>
        <expression>CASE(MONTH({!$Flow.CurrentDate}),
1, &quot;Q1&quot; ,
2, &quot;Q1&quot;,
3, &quot;Q1&quot;,
4, &quot;Q2&quot;,
5, &quot;Q2&quot;,
6, &quot;Q2&quot;,
7, &quot;Q3&quot;,
8, &quot;Q3&quot;,
9, &quot;Q3&quot;,
10, &quot;Q4&quot;,
11, &quot;Q4&quot;,
12, &quot;Q4&quot;,
&quot;ERROR&quot;)</expression>
    </formulas>
    <formulas>
        <name>current_quarter_with_year</name>
        <dataType>String</dataType>
        <expression>TEXT(YEAR({!$Flow.CurrentDate}))&amp;&quot;-&quot;&amp;{!current_quarter}</expression>
    </formulas>
    <formulas>
        <name>days_left_in_month</name>
        <dataType>Number</dataType>
        <expression>(ADDMONTHS(DATE(YEAR(TODAY()), MONTH(TODAY()),1),1)-1)
- 
TODAY()</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>days_left_in_quarter</name>
        <dataType>Number</dataType>
        <expression>IF(MONTH(TODAY()) &lt;= 3, 
DATE(YEAR(TODAY()), 03 , 31 ), 
IF(MONTH(TODAY()) &lt;= 6, 
DATE(YEAR(TODAY()), 06 , 30 ), 
IF(MONTH(TODAY()) &lt;= 9, 
DATE(YEAR(TODAY()), 09 , 30 ), 
DATE(YEAR(TODAY()), 12 , 31 )))) 
- 
TODAY()</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>full_day_name</name>
        <dataType>String</dataType>
        <expression>{!current_month_name}&amp;&quot; &quot;&amp;TEXT(DAY({!$Flow.CurrentDate}))&amp;&quot;, &quot;&amp;TEXT(YEAR({!$Flow.CurrentDate}))</expression>
    </formulas>
