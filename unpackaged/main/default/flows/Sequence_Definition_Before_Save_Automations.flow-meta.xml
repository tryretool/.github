    <formulas>
        <name>account_short_name</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK({!$Record.Target_Account__r.Account_Name_Short__c}),LEFT({!$Record.Target_Account__r.Name},20),{!$Record.Target_Account__r.Account_Name_Short__c})</expression>
    </formulas>
