    </formulas>
    <formulas>
        <name>sequence_name</name>
        <dataType>String</dataType>
        <expression>{!$Record.Sequence_Number__c}&amp;&quot;[&quot;&amp;{!$Record.Short_Name__c}&amp;&quot;]&quot;&amp;
&quot;[&quot;&amp;IF(ISBLANK({!$Record.Assignee__c}),NULL,{!$Record.Assignee__r.Alias}&amp;&quot;]-&quot;)&amp;
IF(ISBLANK({!$Record.Experiment_Name__c}),NULL,&quot;EX[&quot;&amp;{!$Record.Experiment_Name__c}&amp;&quot;:&quot;&amp;TEXT({!$Record.Experiment_Treatment__c})&amp;&quot;]-&quot;)&amp;
TEXT({!$Record.Sourcing_Category__c})&amp;&quot;[&quot;&amp;{!team_tag}&amp;&quot;]-&quot;&amp;
IF(ISPICKVAL({!$Record.Industry__c},&quot;&quot;),NULL,&quot;IN[&quot;&amp;TEXT({!$Record.Industry__c})&amp;&quot;]-&quot;)&amp;
IF(ISPICKVAL({!$Record.Competitor__c},&quot;&quot;),NULL,&quot;C[&quot;&amp;TEXT({!$Record.Competitor__c})&amp;&quot;]-&quot;)&amp;
IF(ISBLANK({!$Record.Target_Account__c}),NULL,&quot;A[&quot;&amp;{!account_short_name}&amp;&quot;]-&quot;)&amp;
&quot;P[&quot;&amp;TEXT({!$Record.Target_Persona_Function__c})&amp;&quot;:&quot;&amp;
TEXT({!$Record.Target_Persona_Level__c})&amp;&quot;]&quot;</expression>
    </formulas>
    <formulas>
        <name>team_tag</name>
        <dataType>String</dataType>
        <expression>CASE({!$Record.Team__c},
 &quot;Account Executives&quot;,&quot;AE:&quot;,
 &quot;Sales Development&quot;,&quot;SD:&quot;,
 &quot;Customer Success&quot;,&quot;CS:&quot;,
 &quot;Marketing Demand Gen&quot;,&quot;MK:&quot;,
 &quot;Deployed Engineering&quot;,&quot;DE:&quot;,
 &quot;Renewal Management&quot;,&quot;RM:&quot;,
 NULL
) &amp;
IF(ISPICKVAL({!$Record.Subteam__c},&quot;&quot;),NULL,TEXT({!$Record.Subteam__c})&amp;&quot;:&quot;)&amp;
IF(ISPICKVAL({!$Record.Segment__c},&quot;&quot;),NULL,TEXT({!$Record.Segment__c})&amp;&quot;:&quot;) &amp;
IF(ISPICKVAL({!$Record.Territory__c},&quot;&quot;),NULL,TEXT({!$Record.Territory__c}))</expression>
    </formulas>
