    <description>Adding checks for Open Journeys from the related Contact to populate associated Journeys and Journey Relationship values.</description>
    <environments>Default</environments>
    <formulas>
        <name>JourneyRelationship</name>
        <dataType>String</dataType>
        <expression>IF(
ISBLANK({!$Record.Contact__r.Current_Open_Journey__c}),
&quot;&quot;,
IF({!$Record.Contact__r.Current_Open_Journey__r.Is_Resting__c}, &quot;Resting&quot;, &quot;Joined&quot;)
)</expression>
    </formulas>
    <formulas>
        <name>MeetingPageSource</name>
        <dataType>String</dataType>
        <expression>IF(FIND(&quot;Meeting Page Source: &quot;, {!PlainTextContext} )=0, &quot;&quot;, 
LEFT(MID(
{!PlainTextContext}, 
FIND(&quot;Meeting Page Source: &quot;, {!PlainTextContext})+LEN(&quot;Meeting Page Source: &quot;), 
1+IF( 
   FIND(  &quot;\n&quot;, {!PlainTextContext}, FIND(&quot;Meeting Page Source: &quot;, {!PlainTextContext} )) = 0
   , LEN({!PlainTextContext})
   , FIND(&quot;\n&quot;,{!PlainTextContext}, FIND(&quot;Meeting Page Source: &quot;, {!PlainTextContext} ))-LEN(&quot;Meeting Page Source: &quot;)-1
)
   - FIND(&quot;Meeting Page Source: &quot;, {!PlainTextContext})
), 254))</expression>
    </formulas>
    <formulas>
        <name>PlainTextContextFormula</name>
        <dataType>String</dataType>
        <expression>JSINHTMLENCODE({!$Record.Moment_Context__c})</expression>
    </formulas>
    <formulas>
        <name>rcid</name>
        <dataType>String</dataType>
        <expression>IF(
    CONTAINS({!PlainTextContextFormula}, &quot;rcid:&quot;),
    MID(
        {!PlainTextContextFormula},
        FIND(&quot;rcid:&quot;, {!PlainTextContextFormula}) + 6,
        18
    ),
    &quot;&quot;
)</expression>
    </formulas>
    <formulas>
        <name>rcid_length</name>
        <dataType>Number</dataType>
        <expression>LEN({!rcid})</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>TestingCRLF</name>
        <dataType>Number</dataType>
        <expression>FIND(&quot;&lt;/p&gt;&quot;, {!PlainTextContext} )</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>ThreeHoursAgo</name>
        <dataType>DateTime</dataType>
        <expression>{!$Flow.InterviewStartTime} - 3/24</expression>
    </formulas>
