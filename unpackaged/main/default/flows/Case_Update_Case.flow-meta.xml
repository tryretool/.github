    <formulas>
        <name>Default_Reopen_Date</name>
        <dataType>Date</dataType>
        <expression>CASE( MOD( DATEVALUE({!$Flow.CurrentDateTime}) - DATE( 1900, 1, 7 ), 7 ),
  0, {!$Flow.CurrentDateTime} + 1 + 0, /* Sun: {!$Flow.CurrentDateTime} + 1 wknd day + 0 days */
  4, {!$Flow.CurrentDateTime} + 1, /* Thurs: {!$Flow.CurrentDateTime} + 1 days */
  5, {!$Flow.CurrentDateTime} + 2 + 1, /* Fri: {!$Flow.CurrentDateTime} + 2 wknd days + 1 days */
  6, {!$Flow.CurrentDateTime} + 1 + 1, /* Sat: {!$Flow.CurrentDateTime} + 1 wknd days + 1 days */
  {!$Flow.CurrentDateTime} + 1 /* Default (Mon/Tue/Wed): {!$Flow.CurrentDateTime} + 2 days */
)</expression>
    </formulas>
    <formulas>
        <name>Default_Reopen_Value</name>
        <dataType>DateTime</dataType>
        <expression>CASE( MOD( DATEVALUE({!$Flow.CurrentDateTime}) - DATE( 1900, 1, 7 ), 7 ),
  0, {!$Flow.CurrentDateTime} + 1 + 0, /* Sun: {!$Flow.CurrentDateTime} + 1 wknd day + 0 days */
  4, {!$Flow.CurrentDateTime} + 1, /* Thurs: {!$Flow.CurrentDateTime} + 1 days */
  5, {!$Flow.CurrentDateTime} + 2 + 1, /* Fri: {!$Flow.CurrentDateTime} + 2 wknd days + 1 days */
  6, {!$Flow.CurrentDateTime} + 1 + 1, /* Sat: {!$Flow.CurrentDateTime} + 1 wknd days + 1 days */
  {!$Flow.CurrentDateTime} + 1 /* Default (Mon/Tue/Wed): {!$Flow.CurrentDateTime} + 2 days */
)</expression>
    </formulas>
